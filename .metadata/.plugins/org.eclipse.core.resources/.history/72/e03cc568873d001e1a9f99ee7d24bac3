package Assignment1;

public class Datatype01 {

	public static void main(String[] args) {
		int a = 10;  // 정수형 변수 a 선언, 10으로 초기화
		short s = 2; // 정수형 변수 s 선언, 2로 초기화
		byte b = 6;	 // 정수형 변수 b 선언, 6으로 초기화
		long l = 125362133223L; // 정수형 변수 l 선언, 해당값으로 초기화
		// 각 변수 출력
		System.out.println(a);
		System.out.println(s);
		System.out.println(b);
		System.out.println(l);
	}

}
