package Assignment1;

public class InformationEx {

	public static void main(String[] args) {
		String name = "옥은지";
		int age = 23;
		long studentNumber = 2325345010L;
		
		System.out.println("이름 : " +name);
		System.out.println("나이 : " +age);
		System.out.println("학번 : " +studentNumber);
		
	}

}
