package javabook;

public class Example01 {

	public static void main(String[] args) {
		final int SPEED = 20;	// 실무에서 종종 쓰임
		int count = 10;
		//SPEED = 40;
		count = 11;
		
		System.out.print(" 상수 SPEED 값은 ");
		System.out.print(SPEED);
		System.out.print(" 상수 count 값은 ");
		System.out.print(count);
				
	}

}
